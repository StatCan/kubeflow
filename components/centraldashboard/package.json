{
    "name": "kubeflow-centraldashboard",
    "version": "0.0.2",
    "description": "Kubeflow Central Dashboard",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack && npm run tslint && npm run build-ts",
        "build-ts": "tsc",
        "build-webpack": "webpack",
        "debug": "npm run build && npm run watch-debug",
        "dev": "concurrently -k -p \"[{name}]\" -n \"Webpack,TypeScript,Express\" -c \"green.bold,yellow.bold,cyan.bold\" \"webpack-dev-server\" \"npm run watch-ts\" \"npm run watch-node\"",
        "dev-debug": "concurrently -k -p \"[{name}]\" -n \"Webpack,TypeScript,Express\" -c \"green.bold,yellow.bold,cyan.bold\" \"webpack-dev-server\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "dev-server-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Express\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "serve": "node dist/server.js",
        "serve-debug": "nodemon --inspect dist/server.js",
        "start": "npm run serve",
        "test": "npm run test-client && npm run test-server",
        "test-client": "karma start --single-run",
        "test-client-debug": "karma start --browsers Chrome",
        "test-e2e": "ts-node --transpile-only node_modules/jasmine/bin/jasmine --config=test/.jasmine.json --reporter=jasmine-console-reporter",
        "test-server": "ts-node --transpile-only node_modules/jasmine/bin/jasmine --config=app/.jasmine.json --reporter=jasmine-console-reporter",
        "ui-test": "cypress open . --e2e",
        "ui-test-ci": "cypress run . --e2e",
        "ui-test-ci-all": "cypress run . --e2e --browser=chrome && cypress run . --e2e --browser=firefox",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "coverage": "nyc npm run test-server && karma start --single-run --reporters coverage-istanbul",
        "watch-node": "nodemon dist/server.js",
        "watch-ts": "tsc -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kubeflow/kubeflow.git"
    },
    "author": "Kubeflow Project",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/kubeflow/kubeflow/issues"
